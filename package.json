{
  "name": "react-redux-typescript",
  "version": "0.0.1",
  "description": "A simple starter kit for react with react router, webpack, hot reloading and eslint",
  "main": "./src/index.tsx",
  "scripts": {
    "stats": "webpack --profile --json > stats.json",
    "test": "mocha --require test/setup.js app/**/*.spec.jsx --compilers js:babel-register",
    "start": "webpack-dev-server --content-base src/ --progress --colors",
    "build": "webpack",
    "prebuild": "rm -rf ./build"
  },
  "jest": {
    "scriptPreprocessor": "./node_modules/babel-jest",
    "unmockedModulePathPatterns": [
      "promise",
      "source-map",
      "react",
      "enzyme",
      "node_modules/react",
      "node_modules/fbjs",
      "node_modules/react-dom",
      "node_modules/react-addons-test-utils",
      "react-dom",
      "react-addons-test-utils",
      "fbjs",
      "cheerio",
      "htmlparser2",
      "underscore",
      "lodash",
      "domhandler",
      "object.assign",
      "define-properties",
      "function-bind",
      "object-keys"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "testPathPattern": "**/*.test.js",
    "testFileExtensions": [
      "js",
      "jsx"
    ],
    "collectCoverage": true,
    "verbose": "true"
  },
  "keywords": [],
  "author": "Johan Nystr√∂m",
  "license": "MIT",
  "dependencies": {
    "@types/redux": "^3.6.31",
    "bootstrap": "^3.3.7",
    "bootswatch": "^3.3.7",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-redux": "^5.0.1",
    "react-router": "^3.0.0",
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "@types/react": "^0.14.55",
    "@types/react-dom": "^0.14.19",
    "@types/react-router": "^2.0.41",
    "@types/redux": "^3.6.0",
    "babel-cli": "^6.7.7",
    "babel-core": "^6.7.7",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-require": "^3.0.0",
    "babel-polyfill": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.8.0",
    "chai": "^3.5.0",
    "concurrently": "^3.1.0",
    "css-loader": "^0.26.1",
    "enzyme": "^2.3.0",
    "eslint": "^3.12.2",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-react": "^6.8.0",
    "expect": "^1.20.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.20.0",
    "jest": "^18.0.0",
    "jest-cli": "^18.0.0",
    "jsdom": "^9.0.0",
    "karma": "^1.3.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.0",
    "karma-firefox-launcher": "^1.0.0",
    "karma-mocha": "^1.0.1",
    "karma-mocha-reporter": "^2.0.3",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "mocha": "^3.2.0",
    "node-sass": "^4.1.1",
    "npm-install-webpack-plugin": "^4.0.4",
    "phantomjs-prebuilt": "^2.1.7",
    "postcss-loader": "^1.2.1",
    "purifycss-webpack-plugin": "^2.0.3",
    "react-addons-test-utils": "^15.0.2",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^4.1.1",
    "sinon": "^1.17.4",
    "style-loader": "^0.13.1",
    "typescript": "^2.2.0-dev.20161228",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "^2.0.0",
    "webpack-validator": "^2.1.2"
  }
}
